    <sql>SELECT distinct
 e.submission_id
, e.mail_part_type
, e.mail_part_content
, s.notification_email
, e.tmp_file_name
, e.pre_transaction_id
FROM fileprocess.email_tmp e, fileprocess.submissions s
where s.idx = e.submission_id
and s.status=&apos;PROCESSED&apos;
